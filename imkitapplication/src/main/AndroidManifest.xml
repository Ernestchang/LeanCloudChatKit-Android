<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.leancloud.imkitapplication" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/lcim_chatkit_name"
        android:name="cn.leancloud.imkitapplication.App"
        android:theme="@style/LCIMKitTheme" >
        <activity
            android:name="cn.leancloud.imkitapplication.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="cn.leancloud.imkitapplication.MainActivity"
                  android:theme="@style/LCIMActivity.NoActionBar"/>
    </application>

</manifest>
